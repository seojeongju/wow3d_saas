{
    "name": "wow-saas",
    "compatibility_date": "2024-04-03",
    "pages_build_output_dir": ".next",
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "wow-archive-db",
            "database_id": "YOUR_D1_DATABASE_ID_HERE"
        }
    ],
    "r2_buckets": [
        {
            "binding": "BUCKET",
            "bucket_name": "wow-archive-bucket"
        }
    ]
}